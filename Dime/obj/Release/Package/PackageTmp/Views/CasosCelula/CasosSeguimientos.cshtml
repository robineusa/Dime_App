@model List<Telmexla.Servicios.DIME.Entity.Ingreso>
@{
    ViewBag.Title = "Casos en Seguimiento";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@if (Session["Seguimientos Sac"] != null)
{
<div class="row">
    <div class="col-md-12" style="width:100%; ">
        <div class="box box-danger">
            <div class="box-body">
                <div class="row">
                    <div class="col-xs-12">
                        <h2 class="page-header" style="margin-top:25px">
                            <i class="fa fa-ticket"></i> Listado de Casos en Seguimiento
                        </h2>
                        <div id="3_ofre" style="background-color:#dcdcdc; padding-left:10px; padding-bottom:10px; margin-top:-10px; margin-left: -10px; padding-right:10px;">
                            <br />
                            <div id="gridViewSeguimientos"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


@section scripts
{
    <script>
        var dataSeguimientos = @Html.Raw(Json.Encode(Model));
    </script>
    
<script src="~/Scripts/CasosCelula/CasosSeguimientos.js?1.0.5"></script>
}
}
else
{
    <div class="row">

        <div class="box box-primary">

            <div class="box-body">

                <div class="row">
                    <div class="col-md-12">
                        <div class="error-page">
                            <h2 class="headline text-red" style="margin-top:-18px;">502</h2>
                            <div class="error-content">
                                <h3><i class="fa fa-warning text-red"></i> Oops! Acceso Denegado.</h3>
                                <p style="text-align:justify;">
                                    La información que se relaciona en esta vista no es posible visualizarse ya que su perfil no posee el acceso necesario.
                                    Por favor solicite al administrador del sistema el acceso correspondiente.
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}