@model List<Telmexla.Servicios.DIME.Entity.MaestroNodo>
@{
    ViewBag.Title = "Lista de Nodos Creados";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@if (Session["Administracion Nodos"] != null)
{
<div class="row">

    <div class="box box-primary">

        <div class="box-body">

            <div class="row">
                <div class="col-md-12">
                    <div id="gridViewNodos"></div>

                </div>
            </div>
        </div>
    </div>


</div>
}
else
{
<div class="row">

    <div class="box box-primary">

        <div class="box-body">

            <div class="row">
                <div class="col-md-12">
                    <div class="error-page">
                        <h2 class="headline text-red" style="margin-top:-18px;">502</h2>
                        <div class="error-content">
                            <h3><i class="fa fa-warning text-red"></i> Oops! Acceso Denegado.</h3>
                            <p style="text-align:justify;">
                                La información que se relaciona en esta vista no es posible visualizarse ya que su perfil no posee el acceso necesario.
                                Por favor solicite al administrador del sistema el acceso correspondiente.
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
}
@section Scripts{
    <script>
        @*var jsonMNodos = @Html.Raw(Json.Encode(Model));*@
        var urlListaKendo = '@Url.Action("ListaNodosCreadosJson", "AdminNodos")';
    </script>

    <script src="~/Scripts/AdminNodos/ListaNodosCreados.js?1.0.4"></script>
}