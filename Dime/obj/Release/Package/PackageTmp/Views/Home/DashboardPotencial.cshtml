
@{
    ViewBag.Title = "Balance Score Card Potencial";
    ViewBag.Subtitle = "Administrador";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@if (Session["Dashboard Potencial"] != null)
{
    <div class="row">
        <div class="col-md-12" style="width:100%; ">
            <div class="box box-danger">
                <div class="box-body">
                    <div class="row">
                        <div class="col-xs-12">
                            <br />
                            <div id="3_ofre" style="background-color:#dcdcdc; padding-left:10px; padding-bottom:10px; margin-top:-10px; margin-left: -10px; padding-right:10px;">
                                <br />
                                <iframe style="width:100%;height:750px;" src="https://app.powerbi.com/view?r=eyJrIjoiNTUwNDBhNGYtYjJhNy00NTQyLWE1Y2YtYmQ5ZmJhNWZmNjdjIiwidCI6IjQ2YmIyMmI4LTRjMmMtNDBmZi04MzYwLTdiNjMzNDgyMTI3OSIsImMiOjR9" frameborder="0" allowFullScreen="true"></iframe>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}
else
{
    <div class="row">

        <div class="box box-primary">

            <div class="box-body">

                <div class="row">
                    <div class="col-md-12">
                        <div class="error-page">
                            <h2 class="headline text-red" style="margin-top:-18px;">502</h2>
                            <div class="error-content">
                                <h3><i class="fa fa-warning text-red"></i> Oops! Acceso Denegado.</h3>
                                <p style="text-align:justify;">
                                    La información que se relaciona en esta vista no es posible visualizarse ya que su perfil no posee el acceso necesario.
