@model Telmexla.Servicios.DIME.Entity.BalanceScoreCard
@{
    ViewBag.Title = "Balanced Score Card Asesor";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">

    <div class="box box-primary">
        @if (Model != null)
        {
        <div class="box-body">

            <div class="row">
                <div class="col-md-12">

                    <style>
                        #chartdiv {
                            width: 100%;
                            height: 500px;
                        }
                    </style>

                    
                    <div id="chartdiv"></div>

                    <table>
                        <tr>
                            <td style="padding:5px;">
                                <div class="info-box">
                                    <span class="info-box-icon bg-red"><i class="ion-ios-telephone"></i></span>
                                    <div class="info-box-content">
                                        <span class="info-box-text">ACD CALLS</span>
                                        <span class="info-box-number">@Html.Label("acdcalls", Model.Acdcalls.ToString())</span>
                                        <span class="info-box-text">META: 0</span>
                                    </div>
                                </div>
                            </td>
                            <td style="padding:5px;">
                                <div class="info-box">
                                    <span class="info-box-icon bg-aqua"><i class="ion-ios-stopwatch"></i></span>
                                    <div class="info-box-content">
                                        <span class="info-box-text">TMO</span>
                                        <span class="info-box-number">@Html.Label("tmo", Model.Tmo.ToString())</span>
                                        <span class="info-box-text">META: 0</span>
                                    </div>
                                </div>
                            </td>
                            <td style="padding:5px;">
                                <div class="info-box">
                                    <span class="info-box-icon bg-gray"><i class="ion-pause"></i></span>
                                    <div class="info-box-content">
                                        <span class="info-box-text">ESPERA</span>
                                        <span class="info-box-number">@Html.Label("espera", Model.Espera.ToString())</span>
                                        <span class="info-box-text">META: 0</span>
                                    </div>
                                </div>
                            </td>
                            <td style="padding:5px;">
                                <div class="info-box">
                                    <span class="info-box-icon bg-orange"><i class="ion-edit"></i></span>
                                    <div class="info-box-content">
                                        <span class="info-box-text">MARCACION</span>
                                        <span class="info-box-number">@Html.Label("marcacion", Model.Marcacion.ToString())</span>
                                        <span class="info-box-text">META: 0</span>
                                    </div>
                                </div>
                            </td>
                            <td style="padding:5px;">
                                <div class="info-box">
                                    <span class="info-box-icon bg-green"><i class="ion-social-usd"></i></span>
                                    <div class="info-box-content">
                                        <span class="info-box-text">Ajustes Correctos</span>
                                        <span class="info-box-number">@Html.Label("ajustes", Model.AjustesCorrectos.ToString())</span>
                                        <span class="info-box-text">META: 0</span>
                                    </div>
                                </div>
                            </td>
                            <td style="padding:5px;">
                                <div class="info-box">
                                    <span class="info-box-icon bg-black"><i class="ion-android-list"></i></span>
                                    <div class="info-box-content">
                                        <span class="info-box-text">PQR correctos</span>
                                        <span class="info-box-number">@Html.Label("pqrcorrectos", Model.PqrsCorrectos.ToString())</span>
                                        <span class="info-box-text">META: 0</span>
                                    </div>
                                </div>
                            </td>
                            <td style="padding:5px;">
                                <div class="info-box">
                                    <span class="info-box-icon bg-blue"><i class="ion-ios-cart"></i></span>
                                    <div class="info-box-content">
                                        <span class="info-box-text">Meta de Ventas</span>
                                        <span class="info-box-number">@Html.Label("metaventas", Model.MetaVentas.ToString())</span>
                                        <span class="info-box-text">META: 0</span>
                                    </div>
                                </div>
                            </td>
                        </tr>
                        <tr>
                            <td style="padding:5px;">
                                <div class="info-box">
                                    <span class="info-box-icon bg-red"><i class="ion-reply-all"></i></span>
                                    <div class="info-box-content">
                                        <span class="info-box-text">REINCIDENCIA ASESOR</span>
                                        <span class="info-box-number">@Html.Label("reincidencia", Model.ReincidenciaAsesor.ToString())</span>
                                        <span class="info-box-text">META: 0</span>
                                    </div>
                                </div>
                            </td>
                            <td style="padding:5px;">
                                <div class="info-box">
                                    <span class="info-box-icon bg-aqua"><i class="ion-android-happy"></i></span>
                                    <div class="info-box-content">
                                        <span class="info-box-text">NPS SOLUCION</span>
                                        <span class="info-box-number">@Html.Label("npssoluctios", Model.NpsSolucion.ToString())</span>
                                        <span class="info-box-text">META: 0</span>
                                    </div>
                                </div>
                            </td>
                            <td style="padding:5px;">
                                <div class="info-box">
                                    <span class="info-box-icon bg-gray"><i class="ion-happy"></i></span>
                                    <div class="info-box-content">
                                        <span class="info-box-text">NPS SATIFACCION</span>
                                        <span class="info-box-number">@Html.Label("npssatisfaccion", Model.NpsSatisfaccion.ToString())</span>
                                        <span class="info-box-text">META: 0</span>
                                    </div>
                                </div>
                            </td>
                            <td style="padding:5px;">
                                <div class="info-box">
                                    <span class="info-box-icon bg-orange"><i class="ion-ios-grid-view"></i></span>
                                    <div class="info-box-content">
                                        <span class="info-box-text">CALIFICACION MEC</span>
                                        <span class="info-box-number">@Html.Label("calificacionmec", Model.CalificacionMec.ToString())</span>
                                        <span class="info-box-text">META: 0</span>
                                    </div>
                                </div>
                            </td>
                            <td style="padding:5px;">
                                <div class="info-box">
                                    <span class="info-box-icon bg-green"><i class="ion-film-marker"></i></span>
                                    <div class="info-box-content">
                                        <span class="info-box-text">ACT CLARO VIDEO</span>
                                        <span class="info-box-number">@Html.Label("acclarovideo", Model.ActivacionesClaroVideo.ToString())</span>
                                        <span class="info-box-text">META: 0</span>
                                    </div>
                                </div>
                            </td>
                            <td style="padding:5px;">
                                <div class="info-box">
                                    <span class="info-box-icon bg-black"><i class="ion-email"></i></span>
                                    <div class="info-box-content">
                                        <span class="info-box-text">ACT CONVENIO FACTURA</span>
                                        <span class="info-box-number">@Html.Label("conveniofactura", Model.ActivacionesConvenioFactura.ToString())</span>
                                        <span class="info-box-text">META: 0</span>
                                    </div>
                                </div>
                            </td>
                            <td style="padding:5px;">
                                <div class="info-box">
                                    <span class="info-box-icon bg-blue"><i class="ion-ios-timer"></i></span>
                                    <div class="info-box-content">
                                        <span class="info-box-text">CALIFICACION TMO</span>
                                        <span class="info-box-number">@Html.Label("calificaciontmo", Model.CalificacionTmo.ToString())</span>
                                        <span class="info-box-text">META: 0</span>
                                    </div>
                                </div>
                            </td>
                        </tr>
                    </table>
                    @Html.Hidden("PuntajeFinal", Model.PuntajeTotal.ToString())
                </div>
            </div>
        </div>
        }
        else
        {
            <div class="error-page">
                <h2 class="headline text-red" style="margin-top:-18px;">501</h2>
                <div class="error-content">
                    <h3><i class="fa fa-warning text-red"></i> Oops! No existe información.</h3>
                    <p style="text-align:justify;">
                        La información que se relaciona en esta vista, no se encuentra disponible para el usuario que inicio sesión, por favor intente más tarde.
                        Usted puede <a href="@Url.Action("ReporteAsesor", "Reportes")">regresar al Inicio</a>
                    </p>

                </div>
            </div>
        }
    </div>

</div>
@section scripts{
    <script src="~/AdminLTE/plugins/Amcharts/amcharts.js"></script>
<script src="~/AdminLTE/plugins/Amcharts/gauge.js"></script>
<script src="~/AdminLTE/plugins/Amcharts/export.min.js"></script>
<link href="~/AdminLTE/plugins/Amcharts/export.css" rel="stylesheet" />
<script src="~/AdminLTE/plugins/Amcharts/none.js"></script>
    <script src="~/Scripts/Reportes/ReporteAsesor.js?1.0.1"></script>
}