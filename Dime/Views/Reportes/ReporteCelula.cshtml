@{
    ViewBag.Title = "Balanced Score Card Celula";
    ViewBag.Subtitle = "Celula";

}

@section Menu {
    @Scripts.Render("~/Scripts/Examples/P500/menu")
}
@if (Session["Reportes Celula"] != null)
{
<div class="row">
    <div class="col-md-12">
        <div class="box box-danger">
            <div class="box-body">
                <div class="row">
                    <div class="col-xs-12">
                        <div class="error-page">
                            <h2 class="headline text-red" style="margin-top:-18px;">500</h2>
                            <div class="error-content">
                                <h3><i class="fa fa-warning text-red"></i> Oops! Página en Construcción.</h3>
                                <p>
                                    La página seleccionada se encuentra en construcción y/o aplicación de cambios, por tal motivo no se encuentra disponible en este momento.
                                    Usted puede <a href="@Url.Action("ReporteCelula", "Reportes")">regresar al Inicio</a>
                                </p>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
}
else
{
<div class="row">
    <div class="box box-primary">
        <div class="box-body">
            <div class="row">
                <div class="col-md-12">
                    <div class="error-page">
                        <h2 class="headline text-red" style="margin-top:-18px;">502</h2>
                        <div class="error-content">
                            <h3><i class="fa fa-warning text-red"></i> Oops! Acceso Denegado.</h3>
                            <p style="text-align:justify;">
                                La información que se relaciona en esta vista no es posible visualizarse ya que su perfil no posee el acceso necesario.
                                Por favor solicite al administrador del sistema el acceso correspondiente.
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
}
