<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Visualizador</title>
    <link href="~/Styles/Slider/css/demo.css" rel="stylesheet" />
    <link href="~/Styles/Slider/css/style.css" rel="stylesheet" />
    <link href="~/Styles/Slider/css/elastislide.css" rel="stylesheet" />
    <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
    <script src="~/Styles/Slider/js/jquery.tmpl.min.js"></script>
    <script src="~/Styles/Slider/js/jquery.elastislide.js"></script>
    <script src="~/Styles/Slider/js/jquery.easing.1.3.js"></script>
    <script>var UrlImagenesActivasjson = '@Url.Action("ListaDeImagenesActivasJson", "OfertasComerciales")';</script>
    <script src="~/Scripts/OfertasComerciales/Visualizador.js?1.0.2"></script>
   

    <script id="img-wrapper-tmpl" type="text/x-jquery-tmpl">
        <div class="rg-image-wrapper">
            {{if itemsCount > 1}}
            <div class="rg-image-nav">
                <a href="#" class="rg-image-nav-prev">Previous Image</a>
                <a href="#" class="rg-image-nav-next">Next Image</a>
            </div>
            {{/if}}
            <div class="rg-image"></div>
            <div class="rg-loading"></div>
            <div class="rg-caption-wrapper">
                <div class="rg-caption" style="display: none;">
                    <p style="color:white;"></p>
                </div>
            </div>
        </div>
    </script>
</head>
<body>
    <div class="container">
        <div class="container">
            <div class="content">
                <div id="rg-gallery" class="rg-gallery">
                    <div class="rg-thumbs">
                        <div class="es-carousel-wrapper">
                            <div class="es-nav">
                                <span class="es-nav-prev">Previous</span>
                                <span class="es-nav-next">Next</span>
                            </div>
                            <div class="es-carousel">
                                <ul id="ContenidoImagenes">
                                    <li>
                                        <a href="#">
                                            <img src="../ImagesClient/Buen Servicio.jpg" data-large="../ImagesClient/Buen Servicio.jpg"  alt="image02" data-description="descripcion 2" data-description2="http://190.144.144.252/Dime" />
                                           
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#">
                                            <img src="../ImagesClient/line up honda nov-01.jpg" data-large="../ImagesClient/line up honda nov-01.jpg" alt="image03" data-description="descripcion 3" data-description2="http://190.144.144.252/Dime" />
                                        </a>
                                    </li>
                                    <li>
                                        <a href="#">
                                            <img src="../ImagesClient/pinpromonoviembre.jpg" data-large="../ImagesClient/pinpromonoviembre.jpg" alt="image03" data-description="descripcion 4" data-description2="http://190.144.144.252/Dime" />
                                        </a>
                                    </li>
                                    
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script src="~/Styles/Slider/js/gallery.js?1.0.3"></script>
</body>
</html>