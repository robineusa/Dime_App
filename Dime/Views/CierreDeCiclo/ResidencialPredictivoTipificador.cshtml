                    <td style="padding:1px;">
                        @Html.TextBoxFor(m => m.GestionResdPredict.Telefono1, new { @class = "form-control input-sm", @Value = Model.ResdPredictInfo.Telefono1, @readonly = "true" })
                    </td>
                    <td style="padding:1px;">
                        @Html.TextBoxFor(m => m.GestionResdPredict.Telefono2, new { @class = "form-control input-sm", @Value = Model.ResdPredictInfo.Telefono2, @readonly = "true" })

                    </td>
                    <td style="padding:1px;">
                        @Html.TextBoxFor(m => m.GestionResdPredict.TelfAdicional1, new { @class = "form-control input-sm", @Value = Model.ResdPredictInfo.Telefono3, @readonly = "true" })
                    </td>
                </tr>

                <tr>

                    <td style="padding:1px;">
                        <small>Nombre Contacto:  </small>
                    </td>
                    <td style="padding:1px;">
                        <small>Servicio:  </small>
                    </td>
                    <td style="padding:1px;">
                        <small>Tipo Cliente:  </small>
                    </td>
                    <td style="padding:1px;">
                        <small>División:  </small>
                    </td>
                    <td style="padding:1px;">
                        <small>Comunidad:  </small>
                    </td>
                    <td style="padding:1px;">
                        <small>Úsuario:</small>
                    </td>
                </tr>
                <tr>

                    <td style="padding:1px;">
                        @Html.TextBoxFor(m => m.GestionResdPredict.NombreContacto, new { @class = "form-control input-sm", @Value = Model.GestionResdPredict.NombreContacto, @readonly = "true" })
                    </td>
                    <td style="padding:1px;">
                        @Html.TextBoxFor(m => m.GestionResdPredict.Servicio, new { @class = "form-control input-sm", @Value = Model.GestionResdPredict.NombreContacto, @readonly = "true" })
