@model Telmexla.Servicios.DIME.Entity.ViewModelResidPredictTipificador
@{
    ViewBag.Title = "Tipificador Residencial Predictivo ";
}

@if (Session["Cierre Ciclo Residencial Predictivo"] != null)
{
    using (Html.BeginForm("ResidencialPredictivoTipificadorPost", "CierreDeCiclo", FormMethod.Post, new { role = "form", onkeydown = "return event.keyCode!=13" }))
    {


        <div role="tabpanel" class="tab-pane tab-dime-first" id="datosAdicionales" style="background-color:#dcdcdc; padding-left:10px; padding-bottom:10px;">
            <table>
                <tr>
                    <td style="padding:1px;"><br /></td>
                </tr>
                <tr>

