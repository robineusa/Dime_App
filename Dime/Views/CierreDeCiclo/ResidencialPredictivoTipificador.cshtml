                    </td>
                    <td style="padding:1px;">
                        @Html.TextBoxFor(m => m.GestionResdPredict.Aliado, new { @class = "form-control input-sm", @Value = Model.GestionResdPredict.Aliado, @readonly = "true" })
                    </td>
                    <td style="padding:1px;">
                        @Html.TextBoxFor(m => m.GestionResdPredict.Canal, new { @class = "form-control input-sm", @Value = Model.BaseMejoramiento.Canal, @readonly = "true" })
                    </td>
                    <td style="padding:1px;">
