                    <td width="200" style="padding:1px;">
                        <small>Teléfono 2:  </small>
                    </td>
                    <td width="200" style="padding:1px;">
                        <small>Tel. Adicional 1:</small>
                    </td>
                </tr>
                <tr>

                    <td style="padding:1px;">
                        @Html.TextBoxFor(m=>m.GestionResdPredict.Cuenta, new { @class = "form-control input-sm", @Value = Model.ResdPredictInfo.Cuenta, @readonly = "true" })
